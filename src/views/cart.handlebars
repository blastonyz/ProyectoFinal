<h1>Lista de Productos del carrito</h1>
<div>
    {{#each cartDb}}
        <div>
            <article>
            <p>{{this._id}}</p>    
            <header>
             <p>{{this.prodId}}</p>   
            </header>    
            <p>{{this.title}}</p>
            <p>{{this.description}}</p>
            <p>{{this.price}}</p>
            <p>{{this.category}}</p>
            <p>{{this.code}}</p>
            <p>{{this.stock}}</p>
            <p>{{this.status}}</p>
            <p>{{this.products.quantity}}</p>
            </article>
        </div>
    {{/each}}
</div>

